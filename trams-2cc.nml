item (FEAT_ROADVEHS, item_roadvehicle_AdelHClass, 256) {
    property {
        name:                           string(STR_NAME_ADELHCLASS);
        introduction_date:              date(1929,01,01);
        model_life:                     112;
        retire_early:                   36;
        vehicle_life:                   28;
        loading_speed:                  40; //should be 5*number of doors
        cost_factor:                    57;
        running_cost_factor:            93;
        speed:                          80*param_speedmod/100 km/h;
        power:                          260 kW;
        weight:                         23;
        cargo_capacity:                 104;
		length:                         8;
        tractive_effort_coefficient:    0.5; //percentage of powered axles
        air_drag_coefficient:           0; //0=default
        //sound_effect:                 ; //not implemented
        //visual_effect:                visual_effect(VISUAL_EFFECT_DEFAULT, 0); //not implemented
        
        reliability_decay:              20;
        climates_available:             ALL_CLIMATES;
        refittable_cargo_classes:       bitmask(CC_PASSENGERS);
        sprite_id:                      SPRITE_ID_NEW_ROADVEH;
        misc_flags:                     bitmask(ROADVEH_FLAG_TRAM, ROADVEH_FLAG_2CC, ROADVEH_FLAG_AUTOREFIT);
        refit_cost:                     0;
        running_cost_base:              RUNNING_COST_ROADVEH;
    }

    graphics {
        purchase:                       spriteset_AdelHClass_purchase;
        //additional_text:              //return string(STR_DESC_);
        cargo_capacity:                 104 / cargo_unit_weight;

        switch_AdelHClass_graphics;
    }
}

//cargo refit options
if (param_refit == 1) {
    item (FEAT_ROADVEHS, item_roadvehicle_AdelHClass) {
        property {
            refittable_cargo_classes:	bitmask(CC_PASSENGERS, CC_MAIL, CC_EXPRESS);
        }
    }
}
else if (param_refit == 2) {
    item (FEAT_ROADVEHS, item_roadvehicle_AdelHClass) {
        property {
            refittable_cargo_classes:	ALL_NORMAL_CARGO_CLASSES;
        }
    }
}

//reduced model life (default)
if (param_modellife == 0) {
    item (FEAT_ROADVEHS, item_roadvehicle_AdelHClass) {
        property {
            model_life:                 46;
			retire_early:               36;
        }
    }
}
